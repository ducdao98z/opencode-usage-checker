{
    "name": "opencode-usage-checker",
    "version": "0.0.1",
    "description": "Check all your AI subscription quotas in one command. Supports OpenAI, Zhipu AI, Z.ai, and Minimax. More platforms coming soon.",
    "type": "module",
    "main": "dist/plugin/mystatus.js",
    "types": "dist/plugin/mystatus.d.ts",
    "exports": {
        ".": {
            "import": "./dist/plugin/mystatus.js",
            "types": "./dist/plugin/mystatus.d.ts"
        }
    },
    "files": [
        "dist",
        "command",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc",
        "typecheck": "tsc --noEmit",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write .",
        "prepublishOnly": "npm run lint && npm run build"
    },
    "keywords": [
        "opencode",
        "plugin",
        "quota",
        "status",
        "openai",
        "zai",
        "claude",
        "minimax",
        "anthropic",
        "copilot"
    ],
    "author": "javazys",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ducdao98z/opencode-usage-checker.git"
    },
    "bugs": {
        "url": "https://github.com/ducdao98z/opencode-usage-checker/issues"
    },
    "homepage": "https://github.com/ducdao98z/opencode-usage-checker#readme",
    "dependencies": {
        "@opencode-ai/plugin": "^1.1.0",
        "playwright": "^1.58.2"
    },
    "devDependencies": {
        "@types/node": "^22.0.0",
        "@typescript-eslint/eslint-plugin": "^8.52.0",
        "@typescript-eslint/parser": "^8.52.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "globals": "^17.0.0",
        "prettier": "^3.7.4",
        "typescript": "^5.8.0"
    },
    "peerDependencies": {
        "@opencode-ai/plugin": "^1.1.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
